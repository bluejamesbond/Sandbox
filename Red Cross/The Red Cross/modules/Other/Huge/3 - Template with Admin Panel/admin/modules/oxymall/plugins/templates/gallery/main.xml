<MediaGallery>
	
	<settings
	
		distanceBetweenMenuButtons="{SET_DISTANCEBETWEENMENUBUTTONS}"
		
		thumbWidth="{SET_THUMBWIDTH}"
		
		thumbHeight="{SET_THUMBHEIGHT}"
		
		horizontalThumbSpace="{SET_HORIZONTALTHUMBSPACE}"
		
		verticalThumbSpace="{SET_VERTICALTHUMBSPACE}"

		scrollerBehavior="{SET_SCROLLERBEHAVIOR}"
	
	></settings>

