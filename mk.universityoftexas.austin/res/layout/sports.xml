<?xml version="1.0" encoding="utf-8"?>
<mk.universityoftexas.austin.LightingLayoutRelative xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/log"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/sports_bg"
        android:orientation="vertical" >

        <mk.universityoftexas.austin.OverlayProgressBar
            android:id="@+id/progress"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="10dp" />

        <LinearLayout
            android:id="@+id/sdfsdfsd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="-80dp"
            android:layout_marginTop="25dp"
            android:background="@drawable/sports_spiral"
            android:orientation="vertical" >

            <RelativeLayout
                android:id="@+id/relativeLayout1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:paddingLeft="50dp" android:paddingRight="80dp">

                <HorizontalScrollView
                    android:id="@+id/lsdfdsf"
                    android:layout_height="wrap_content"
                    android:scrollbars="none" android:layout_width="wrap_content" android:background="#00000000" android:focusable="false" android:focusableInTouchMode="false" android:fadingEdgeLength="0dp">

                    <LinearLayout
                        android:id="@+id/linearLayout3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" >

                        <LinearLayout
                            android:id="@+id/linearLayout2"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="vertical" >

                            <RelativeLayout
                                android:id="@+id/sports_scores"
                                android:layout_width="89dp"
                                android:layout_height="81dp"
                                android:background="@drawable/sports_sticky_1"
                                android:clickable="true"
                                android:onClick="onClick" >
                            </RelativeLayout>

                            <ImageView
                                android:id="@+id/sports_scores_arrow"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/selection" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/linearLayout6"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="vertical" >

                            <RelativeLayout
                                android:id="@+id/sports_roster"
                                android:layout_width="89dp"
                                android:layout_height="81dp"
                                android:background="@drawable/sports_sticky_2"
                                android:clickable="true"
                                android:onClick="onClick" >
                            </RelativeLayout>

                            <ImageView
                                android:id="@+id/sports_roster_arrow"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/selection" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/linearLayout7"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="vertical" >

                            <RelativeLayout
                                android:id="@+id/sports_news"
                                android:layout_width="89dp"
                                android:layout_height="81dp"
                                android:background="@drawable/sports_sticky_4"
                                android:clickable="true"
                                android:onClick="onClick" >
                            </RelativeLayout>

                            <ImageView
                                android:id="@+id/sports_news_arrow"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/selection" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/linearLayout8"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="vertical" >

                            <RelativeLayout
                                android:id="@+id/sports_schedule"
                                android:layout_width="89dp"
                                android:layout_height="81dp"
                                android:background="@drawable/sports_sticky_3"
                                android:clickable="true"
                                android:onClick="onClick" >
                            </RelativeLayout>

                            <ImageView
                                android:id="@+id/sports_schedule_arrow"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:src="@drawable/selection" />
                        </LinearLayout>
                    </LinearLayout>
                </HorizontalScrollView>

            </RelativeLayout>

            <ImageButton
                android:id="@+id/refresh"
                android:layout_width="74dp"
                android:layout_height="33dp"
                android:layout_marginLeft="80dp"
                android:background="@drawable/refresh"
                android:clickable="true"
                android:onClick="onClick" />

            <ScrollView
                android:id="@+id/sports_scroll_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="80dp"
                android:layout_marginLeft="70dp"
                android:layout_marginRight="90dp"
                android:layout_marginTop="10dp"
                android:scrollbars="none" >

                <LinearLayout
                    android:id="@+id/sports_scroll_view_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >
                </LinearLayout>
            </ScrollView>

        </LinearLayout>

    </LinearLayout>

</mk.universityoftexas.austin.LightingLayoutRelative>