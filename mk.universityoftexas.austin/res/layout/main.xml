<?xml version="1.0" encoding="utf-8"?>
<mk.universityoftexas.austin.LightingLayoutRelative xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/log"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/home_bg"
        android:orientation="vertical" >

        <mk.universityoftexas.austin.OverlayProgressBar
            android:id="@+id/progress"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="10dp" />

        <LinearLayout
            android:id="@+id/container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="-10dp"
                android:orientation="horizontal" >

                <RelativeLayout
                    android:id="@+id/relativeLayout1"
                    android:layout_width="200dp"
                    android:layout_height="174dp"
                    android:layout_marginBottom="15dp"
                    android:layout_marginLeft="-40dp"
                    android:layout_marginTop="80dp"
                    android:background="@drawable/polaroid" >

                    <ImageView
                        android:id="@+id/home_news_image"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginBottom="20dp"
                        android:layout_marginLeft="-20dp"
                        android:layout_marginRight="25dp"
                        android:layout_marginTop="20dp"
                        android:clickable="true"
                        android:src="@drawable/sample_news" />
                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/linearLayout4"
                    android:layout_width="fill_parent"
                    android:layout_height="150dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="-10dp"
                    android:layout_marginTop="180dp"
                    android:orientation="vertical"
                    android:paddingLeft="5dp" >

                    <ImageView
                        android:id="@+id/imageView1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/home_todays_news" />

                    <LinearLayout
                        android:id="@+id/LinearLayout01"
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:layout_marginTop="5dp"
                        android:background="#000000" >
                    </LinearLayout>

                    <TextView
                        android:id="@+id/home_news_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="5dp"
                        android:layout_marginTop="-2dp"
                        android:paddingTop="5sp"
                        android:text="Lorem ipsum dolor sit amet, consectetur adipiscing elit. Nunc massa sem, malesuada eget convallis ac, fermentum non velit."
                        android:textColor="#000000"
                        android:textSize="11sp" />

                    <LinearLayout
                        android:id="@+id/linearLayout1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp" >

                        <mk.universityoftexas.austin.HighlightedTextView
                            android:id="@+id/home_news_1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:clickable="true"
                            android:onClick="onClick"
                            android:paddingBottom="5dp"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:paddingTop="5dp"
                            android:text="1"
                            android:textColor="#000000" />

                        <mk.universityoftexas.austin.HighlightedTextView
                            android:id="@+id/home_news_2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:clickable="true"
                            android:onClick="onClick"
                            android:paddingBottom="5dp"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:paddingTop="5dp"
                            android:text="2"
                            android:textColor="#000000" />

                        <mk.universityoftexas.austin.HighlightedTextView
                            android:id="@+id/home_news_3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:clickable="true"
                            android:onClick="onClick"
                            android:paddingBottom="5dp"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:paddingTop="5dp"
                            android:text="3"
                            android:textColor="#000000" />

                        <mk.universityoftexas.austin.HighlightedTextView
                            android:id="@+id/home_news_4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:clickable="true"
                            android:onClick="onClick"
                            android:paddingBottom="5dp"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:paddingTop="5dp"
                            android:text="4"
                            android:textColor="#000000" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:orientation="vertical" >

                <ScrollView
                    android:id="@+id/scrollView1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingBottom="5dp"
                    android:scrollbars="none" >

                    <LinearLayout
                        android:id="@+id/linearLayout6"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical" >

                        <mk.universityoftexas.austin.HighlightedImageButton
                            android:id="@+id/home_utacess_button"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:background="@drawable/home_utacess_button"
                            android:onClick="onClick"
                            android:visibility="invisible" />

                        <mk.universityoftexas.austin.HighlightedImageButton
                            android:id="@+id/home_blogs_button"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:background="@drawable/home_blogs_button"
                            android:onClick="onClick"
                            android:visibility="invisible" />

                        <mk.universityoftexas.austin.HighlightedImageButton
                            android:id="@+id/home_news_button"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5dp"
                            android:background="@drawable/home_news_button"
                            android:onClick="onClick"
                            android:visibility="invisible" />

                        <mk.universityoftexas.austin.HighlightedImageButton
                            android:id="@+id/home_sports_button"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right"
                            android:layout_marginBottom="5dp"
                            android:background="@drawable/home_sports_button"
                            android:onClick="onClick"
                            android:visibility="invisible" />

                        <mk.universityoftexas.austin.HighlightedImageButton
                            android:id="@+id/home_media_button"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right"
                            android:layout_marginBottom="5dp"
                            android:background="@drawable/home_media_button"
                            android:onClick="onClick"
                            android:visibility="invisible" />

                        <mk.universityoftexas.austin.HighlightedImageButton
                            android:id="@+id/home_campus_life_button"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right"
                            android:layout_marginBottom="5dp"
                            android:background="@drawable/home_campus_life_button"
                            android:onClick="onClick"
                            android:visibility="invisible" />

                        <LinearLayout
                            android:id="@+id/linearLayout5"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:orientation="vertical" >

                            <mk.universityoftexas.austin.HighlightedImageButton
                                android:id="@+id/home_alerts_safety_button"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="10dp"
                                android:layout_marginTop="-6dp"
                                android:background="@drawable/home_alerts_safety_button"
                                android:onClick="onClick"
                                android:visibility="invisible" />
                        </LinearLayout>
                    </LinearLayout>
                </ScrollView>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</mk.universityoftexas.austin.LightingLayoutRelative>