<?xml version="1.0" encoding="utf-8"?>
<mk.universityoftexas.austin.LightingLayoutRelative xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/log"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/alerts_safety_menu_bg"
        android:orientation="vertical" >

        <mk.universityoftexas.austin.OverlayProgressBar
            android:id="@+id/progress"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="10dp" />

        <RelativeLayout
            android:id="@+id/rl2"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" >

            <RelativeLayout
                android:id="@+id/rl"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="80dp"
                android:layout_marginLeft="-20dp"
                android:layout_marginRight="-150dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/alerts_safety_menu_book" >

                <LinearLayout
                    android:id="@+id/container"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="35dp"
                    android:layout_marginTop="55dp"
                    android:orientation="vertical"
                    android:paddingRight="40dp" >

                    <mk.universityoftexas.austin.HighlightedTextView
                        android:id="@+id/alerts_safety_autoassist"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:background="#00000000"
                        android:onClick="onClick"
                        android:paddingRight="20dp"
                        android:text="Longhorn Auto Assist" android:clickable="true" android:textColor="#000000" android:visibility="invisible"/>

                    <mk.universityoftexas.austin.HighlightedTextView
                        android:id="@+id/alerts_safety_behavior"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:background="#00000000"
                        android:onClick="onClick"
                        android:paddingRight="20dp"
                        android:text="24hr Behavioral Concerns" android:clickable="true" android:textColor="#000000" android:visibility="invisible"/>

                    <mk.universityoftexas.austin.HighlightedTextView
                        android:id="@+id/alerts_safety_counsel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:background="#00000000"
                        android:onClick="onClick"
                        android:paddingRight="20dp"
                        android:text="24hr Counseling" android:clickable="true" android:textColor="#000000" android:visibility="invisible"/>

                    <mk.universityoftexas.austin.HighlightedTextView
                        android:id="@+id/alerts_safety_environ"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:background="#00000000"
                        android:onClick="onClick"
                        android:paddingRight="20dp"
                        android:text="Environmental Safety" android:clickable="true" android:textColor="#000000" android:visibility="invisible"/>

                    <mk.universityoftexas.austin.HighlightedTextView
                        android:id="@+id/alerts_safety_lostfound"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:background="#00000000"
                        android:onClick="onClick"
                        android:paddingRight="20dp"
                        android:text="Lost and Found Hotline" android:clickable="true" android:textColor="#000000" android:visibility="invisible"/>

                    <mk.universityoftexas.austin.HighlightedTextView
                        android:id="@+id/alerts_safety_nurse"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:background="#00000000"
                        android:onClick="onClick"
                        android:paddingRight="20dp"
                        android:text="24hr Nurse Advice" android:clickable="true" android:textColor="#000000" android:visibility="invisible"/>

                    <mk.universityoftexas.austin.HighlightedTextView
                        android:id="@+id/alerts_safety_utpolice"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dp"
                        android:background="#00000000"
                        android:onClick="onClick"
                        android:paddingRight="20dp"
                        android:text="UT Police" android:clickable="true" android:textColor="#000000" android:visibility="invisible"/>
                </LinearLayout>

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_alignRight="@+id/container"
                    android:src="@drawable/pennies" />
            </RelativeLayout>
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="-80dp"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="162dp"
                android:layout_height="162dp"
                android:src="@drawable/headphones" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="-30dp"
                android:layout_marginLeft="53dp"
                android:layout_marginTop="-10dp"
                android:src="@drawable/cellphone" />
        </LinearLayout>
    </LinearLayout>

</mk.universityoftexas.austin.LightingLayoutRelative>